<div>
  <h2>Users that match your search:</h2>
    <% if @users == [] %>
        <h4>There are no users that match your search.</h4>
    <% else %>
      <% @users.each do |user| %>
      <h3><%= link_to "Add #{user.username}", friendships_path(:to_id => user), :method => :post %><h3>
      <% end %>
    <% end %>
</div>
